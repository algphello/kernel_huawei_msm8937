		qcom,gpio-reset = <1>;
		qcom,gpio-af-pwdm = <2>;
		qcom,gpio-cam-id = <3>;
		qcom,gpio-req-tbl-num = <0 1 2 3>;
		qcom,gpio-req-tbl-flags = <1 0 0 1>;
		qcom,gpio-req-tbl-label = "CAMIF_MCLK0",
			"CAM_RESET0",
